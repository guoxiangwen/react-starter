{
  "name": "react-demo",
  "version": "1.0.0",
  "description": "react",
  "main": "main.js",
  "author": "guoxiangwen",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src/**/*.jsx",
    "e2e": "node test/e2e/runner.js",
    "unit": "jest",
    "test": "jest",
    "build": "rimraf build && webpack --config ./scripts/webpack.config.pro.js  --display-error-details --progress --color",
    "start": "webpack-dev-server  --config ./scripts/webpack.config.dev.js",
    "deploy": "npm run build && sh scripts/deploy.sh"
  },
  "dependencies": {
    "@types/react": "^15.0.30",
    "@types/react-dom": "^15.5.0",
    "@types/react-router": "^3.0.5",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "^3.0.5"
  },
  "devDependencies": {
    "@types/jest": "^20.0.1",
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.16.0",
    "chromedriver": "^2.30.1",
    "css-loader": "^0.26.4",
    "enzyme": "^2.7.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.24.1",
    "jest": "^20.0.4",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "nightwatch": "^0.9.16",
    "postcss-loader": "^2.0.6",
    "react-addons-test-utils": "^15.6.0",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.1",
    "selenium-server": "^3.3.1",
    "style-loader": "^0.13.2",
    "ts-jest": "^20.0.6",
    "ts-loader": "^2.1.0",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1",
    "webpack-browser-plugin": "^1.0.20",
    "webpack-dev-server": "^2.5.0",
    "webpack-manifest-plugin": "^1.1.0"
  },
  "jest": {
    "verbose": true,
    "bail": true,
    "automock": true,
    "notify": true,
    "modulePaths": [
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverage": true,
    "coverageDirectory": "test/unit/coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/e2e/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/test/e2e/"
    ]
  }
}