  "jest": {
    "verbose": true,
    "bail": true,
    "automock": true,
    "notify": true,
    "modulePaths": [
      "src"
    ],
    "collectCoverage": true,
    "coverageDirectory": "test/unit/coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/e2e/"
