    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "awesome-typescript-loader": "^3.2.3",
    "body-parser": "^1.17.2",
    "clean-webpack-plugin": "^0.1.16",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.28.5",
    "debug": "^3.0.1",
    "ejs": "^2.5.7",
    "enzyme": "^2.9.1",
    "express": "^4.15.4",
